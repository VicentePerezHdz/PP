            </div>

            <nav>
                <ul id="menu">

                    <li>@Html.ActionLink("Inicio", "Index", "Sftp")</li>
                    <li>@Html.ActionLink("Consultar", "Consultar", "Sftp")</li>
                    <li>@Html.ActionLink("Calificaciones", "ObtenerCalificaciones", "Sftp")</li>

                    <li style="float:right">@Html.ActionLink("Cerrar Sesión", "CerrarSesion", "Login")</li>
                </ul>
            </nav>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer >
            Create Page By Vicente de Jesus ©
        </footer>
    </div>
</body>
</html>

